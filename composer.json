{
    "name": "jlaswell/simple-cache",
    "type": "library",
    "description": "Simple caching based on the proposed PSR-16",
    "keywords": [
        "jlaswell",
        "simple-cache",
        "cache",
        "PSR-16"
    ],
    "homepage": "https://github.com/jlaswell/simple-cache",
    "license": "MIT",
    "authors": [
        {
            "name": "John Laswell",
            "email": "john.n.laswell@gmail.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/php-fig/simplecache"
        }
    ],
    "require": {
        "php": "~7.0",
        "predis/predis": "^1.1",
        "psr/simple-cache": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Jlaswell\\SimpleCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jlaswell\\SimpleCache\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "format": "phpcbf --standard=psr2 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
